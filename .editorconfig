# EditorConfig is awesome: https://EditorConfig.org

root = true

# noinspection EditorConfigKeyCorrectness
[*.{ts,xml,html,http,json}]
charset = utf-8
indent_style = space
indent_size = 2
continuation_indent_size = 8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true
max_line_length = 120
